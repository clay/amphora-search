<url>
  {{#if loc}}<loc>{{loc}}</loc>{{/if}}
  {{#if lastmod}}<lastmod>{{lastmod}}</lastmod>{{/if}}
  {{#if changefreq}}<changefreq>{{changefreq}}</changefreq>{{/if}}
  {{#if priority}}<priority>{{priority}}</priority>{{/if}}
  {{#each images}}
  <image:image>
    {{#if loc}}<image:loc>{{loc}}</image:loc>{{/if}}
    {{#if caption}}<image:caption>{{caption}}</image:caption>{{/if}}
  </image:image>
  {{/each}}
</url>
